{
  "name": "@cmdlucas/react-mediaquery",
  "version": "0.0.1",
  "description": "A React helper library built upon semantic-ui to allow serving different components to different screens easily on server rendering or in browsers",
  "main": "./bin/index.js",
  "types": "./bin/types/index.d.ts",
  "scripts": {
    "start": "node ./bin/index.js",
    "production": "webpack && node ./bin/index.js",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/index.tsx",
    "test": "jest",
    "watch-test": "npm run test -- --watchAll",
    "build": "webpack",
    "prod": "npm run production"
  },
  "keywords": [
    "semantic-ui",
    "media query",
    "responsive"
  ],
  "author": "Caleb I. Lucas <caleb@lucasbin.com> (https://caleb.lucasbin.com)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-typescript": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.7.1",
    "jest": "^24.7.1",
    "lodash": "^4.17.11",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^5.3.3",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.4.4",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "prop-types": "^15.7.2",
    "mobile-detect": "^1.4.3",
    "semantic-ui-react": "^0.86.0"
  }
}
